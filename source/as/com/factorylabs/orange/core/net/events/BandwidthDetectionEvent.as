package com.factorylabs.orange.core.net.events{	import flash.events.Event;		/**	 * Event object for sending information on BandwidthDetection events.	 * 	 * Copyright 2008 by Factory Design Labs, All Rights Reserved.	 * <a href="http://www.factorylabs.com/">www.factorylabs.com</a>	 *	 * @author		Grant	 * @version		1.0.0 :: Apr 2, 2008	 */
	public class BandwidthDetectionEvent		extends Event 	{		public static const DETECTION_COMPLETE:String = "BandwidthDetectionEvent.complete";		public static const DETECTION_ERROR:String = "BandwidthDetectionEvent.error";				/**		 * Stores the current bandwidth, in Kbps, associated with this event. 		 */		public var bandwidth:Number;				/**		 * @param type			Sting value of the event type.		 * @param kbps			[Optional] Number value of the current bandwidth, in Kpbs.		 * @param bubbles		[Optional] Boolean flag to indicate if this event should bubble or not.		 * @param cancelable 	[Optional] Boolean flag to indicate if this event is cancellable. 		 */		public function BandwidthDetectionEvent(type:String, kbps:Number=NaN, bubbles:Boolean=false, cancelable:Boolean=false )		{			super( type, bubbles, cancelable );			bandwidth = kbps;		}				/**		 * Creates a copy of this even object and returns the new object. 		 */		override public function clone():Event		{			return new BandwidthDetectionEvent( this.type, this.bandwidth, this.bubbles, this.cancelable );		}	}}