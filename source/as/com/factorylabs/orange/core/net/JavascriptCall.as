package com.factorylabs.orange.core.net{		/** 	 * Provides a DTO which stores information on a particular javascript function call. 	 *  	 * Copyright 2008 by Factory Design Labs, All Rights Reserved. 	 * <a href="http://www.factorylabs.com/">www.factorylabs.com</a> 	 *  	 * @author		Grant 	 * @version		1.0.0 :: May 5, 2008 	 */	public class JavascriptCall 	{		/**		 * String value of the javascript function to call.		 */		public var functionName:String;				/**		 * Callback method to handle returned javascript data.		 */		public var callback:Function;				/**		 * Array of arguments to send to the javascript function.		 */		public var args:Array;				/**		 * Indicates the importance of this call. Higher priority values are handled before low priority values. 		 */		public var priority:int;	}}