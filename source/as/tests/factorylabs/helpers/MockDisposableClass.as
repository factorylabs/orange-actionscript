package tests.factorylabs.helpers{	import com.factorylabs.orange.core.gc.IDisposable;		/**	 * Basic necessities to mock a disposable class. 	 *  	 * <p>Copyright 2009 by Factory Design Labs, All Rights Reserved.</p> 	 * <a href="http://www.factorylabs.com/">www.factorylabs.com</a> 	 *  	 * @author		Matthew Kitt 	 * @version		1.0.0 :: May 26, 2009 	 */	public class MockDisposableClass		implements IDisposable	{		private var _isDisposed		:Boolean;				public function get isDisposed() :Boolean { return _isDisposed; }		public function set isDisposed( v_isDisposed :Boolean ) :void		{			_isDisposed = v_isDisposed;		}		public function MockDisposableClass()		{			_isDisposed = false;		}		public function toString() :String 		{			return "tests.factorylabs.helpers.MockDisposableClass";		}				public function dispose() :void		{			_isDisposed = true;		}	}}