package tests.factorylabs.fluint.ui{	import tests.digitalprimates.fluint.monitor.TestCaseResult;	import tests.digitalprimates.fluint.ui.TestRunner;	import tests.factorylabs.fluint.events.TestResultEvent;		/**	 * The FTestRunner class is responsible for ensuring every Test Method in every TestCase in every TestSuite is executed 	 * and the results are aggragated.  	 *  	 * <p>Copyright 2009 by Factory Design Labs, All Rights Reserved.</p> 	 * <a href="http://www.factorylabs.com/">www.factorylabs.com</a> 	 *  	 * @author		Matthew Kitt 	 * @version		1.0.0 :: Mar 28, 2009 	 */	public class FTestRunner		extends TestRunner	{		public static const TESTS_COMPLETE		:String = TestRunner.TESTS_COMPLETE;		private var _currentCaseResult			:TestCaseResult = null;		
		public function FTestRunner()		{			super();		}				/**		 * Only dispatches a test case result prior to running the test methods for the related case and suite. 		 * @inheritDoc		 */		override protected function runSetup() :void		{			var caseResult :TestCaseResult = testMonitor.getTestCaseResult( testCase );			if( _currentCaseResult != caseResult )			{				_currentCaseResult = caseResult;				dispatchEvent( new TestResultEvent( TestResultEvent.TEST_CASE_RESULT, _currentCaseResult ) );			}			super.runSetup();		}				/**		 * Adds dispatching capabilities after a test method has finished running.		 * @inheritDoc		 */		override protected function runTearDown() :void		{			super.runTearDown();			dispatchEvent( new TestResultEvent( TestResultEvent.TEST_METHOD_RESULT, testMonitor.getTestMethodResult( testMethod.method ) ) );		}	}}