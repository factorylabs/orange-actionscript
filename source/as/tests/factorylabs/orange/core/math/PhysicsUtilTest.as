package tests.factorylabs.orange.core.math{	import tests.digitalprimates.fluint.tests.TestCase;
	
	import com.factorylabs.orange.core.math.PhysicsUtil;	
	/** 	 * Test methods for the PhysicsUtil test case. 	 *  	 * <p>Copyright 2009 by Factory Design Labs, All Rights Reserved.</p> 	 * <a href="http://www.factorylabs.com/">www.factorylabs.com</a> 	 *  	 * @author		Matthew Kitt 	 * @version		1.0.0 :: Mar 16, 2009 	 */	public class PhysicsUtilTest		extends TestCase	{			public function PhysicsUtilTest()		{			super();		}				override public function toString() :String 		{			return "tests.factorylabs.core.math.PhysicsUtilTest";		}				public function test_getAngleToTarget() :void		{			assertEquals( 141, Math.round( PhysicsUtil.getAngleToTarget( 10, 20, 50, 70 ) ) );		}				public function test_constrainAngle() :void		{			assertEquals( 360, PhysicsUtil.constrainAngle( 720 ) );			assertEquals( -360, PhysicsUtil.constrainAngle( -720 ) );		}				public function test_getHypotenuse() :void		{			assertEquals( 22, Math.round( PhysicsUtil.getHypotenuse( 10, 20, 50, 70 ) ) );		}	}}