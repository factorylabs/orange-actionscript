package tests.factorylabs.orange.core.gc{	import tests.digitalprimates.fluint.tests.TestCase;		import com.factorylabs.orange.core.gc.JanitorInterval;		import flash.utils.setInterval;		/** 	 * Test methods for the JanitorInterval test case. 	 *  	 * <p>Copyright 2009 by Factory Design Labs, All Rights Reserved.</p> 	 * <a href="http://www.factorylabs.com/">www.factorylabs.com</a> 	 *  	 * @author		Matthew Kitt 	 * @version		1.0.0 :: May 28, 2009 	 */	public class JanitorIntervalTest		extends TestCase	{		private var _janitor	:JanitorInterval;		private var _interval	:uint;		public function JanitorIntervalTest()		{			super();		}				override public function toString() :String 		{			return "tests.factorylabs.core.gc.JanitorIntervalTest";		}				override protected function setUp() :void		{			_janitor = new JanitorInterval( this );		}				override protected function tearDown() :void		{			_janitor.cleanUp();			_janitor = null;		}				public function test_addIntervalID() :void		{			_interval = setInterval( tick, 1000, "Mansfield" );			_janitor.addIntervalID( _interval );			assertNotNull( _janitor.intervalIDs[ _interval ] );			_janitor.cleanUpIntervalIDs();		}				[Test(description="Removing an interval does not clear it, only pulls it out of the dictionary object.", tracker_id="")]		public function test_removeIntervalID() :void		{			_interval = setInterval( tick, 1000, "Mansfield" );			_janitor.addIntervalID( _interval );			_janitor.removeIntervalID( _interval );			_janitor.cleanUpIntervalIDs();		}				public function test_cleanUpIntervalIDs() :void		{			_interval = setInterval( tick, 1000, "Mansfield" );			_janitor.addIntervalID( _interval );			_janitor.cleanUpIntervalIDs();		}		private function tick( msg :String = null ) :void		{		}	}}