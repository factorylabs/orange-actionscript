		override protected function tearDown() :void		{			_janitor.cleanUp();			_janitor = null;		}				public function test_addIntervalID() :void		{			_interval = setInterval( tick, 1000, "Mansfield" );			_janitor.addIntervalID( _interval );			assertNotNull( _janitor.intervalIDs[ _interval ] );			_janitor.cleanUpIntervalIDs();		}				[Test(description="Removing an interval does not clear it, only pulls it out of the dictionary object.", tracker_id="")]		public function test_removeIntervalID() :void		{			_interval = setInterval( tick, 1000, "Mansfield" );			_janitor.addIntervalID( _interval );			_janitor.removeIntervalID( _interval );			_janitor.cleanUpIntervalIDs();		}				public function test_cleanUpIntervalIDs() :void		{			_interval = setInterval( tick, 1000, "Mansfield" );			_janitor.addIntervalID( _interval );			_janitor.cleanUpIntervalIDs();		}		private function tick( msg :String = null ) :void		{		}	}}